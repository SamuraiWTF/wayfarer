[mysqld]
max_connections        = 10
key_buffer_size        = 16K
max_allowed_packet     = 1M
thread_stack           = 192K
table_open_cache       = 4
sort_buffer_size       = 64K
read_buffer_size       = 256K
read_rnd_buffer_size   = 256K
net_buffer_length      = 2K
thread_cache_size      = 8
innodb_buffer_pool_size = 4M
query_cache_limit       = 1M
query_cache_size        = 4M
skip-log-bin
skip-log-slave-updates